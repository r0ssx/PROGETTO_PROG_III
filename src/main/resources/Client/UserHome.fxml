<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="Client.Controllers.HomeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText" text="Authentication:"/>
    <Button text="User Login" onAction="#userLoginClick"/>
    <Button text="User Register" onAction="#userRegisterClick"/>
    <Button text="Admin Login" onAction="#adminLoginClick"/>
</VBox>

