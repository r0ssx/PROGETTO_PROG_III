    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" text="Admin Home" />
    <Button onAction="#insertClick" text="Inserisci prodotto" />
   <Button layoutX="30.0" layoutY="67.0" onAction="#modifyClick" text="Modifica prodotto" />
